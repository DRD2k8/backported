{
  "schemaVersion": 1,
  "id": "backported",
  "version": "${version}",
  "name": "Backported",
  "description": "A mod that backports everything from other versions to the most popular modding versions.",
  "authors": [
    "Dandi2k8"
  ],
  "contact": {
    "homepage": "https://fabricmc.net/",
    "sources": "https://github.com/FabricMC/fabric-example-mod"
  },
  "license": "CC0-1.0",
  "icon": "icon.png",
  "environment": "*",
  "entrypoints": {
    "main": [
      "com.drd.backported.fabric.BackportedFabric"
    ],
    "client": [
      "com.drd.backported.fabric.client.BackportedFabricClient"
    ]
  },
  "mixins": [
    "backported.mixins.json",
    "backported.fabric.mixins.json"
  ],
  "depends": {
    "fabricloader": ">=0.18.4",
    "minecraft": "~1.20.1",
    "java": ">=17",
    "architectury": ">=9.2.14",
    "fabric-api": "*",
    "trimmed": ">=2.1.0",
    "sherdsapi": ">=4.0.4"
  },
  "suggests": {
    "another-mod": "*"
  }
}
